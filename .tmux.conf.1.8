# TMUX v1.8-compatible configuration

unbind C-b
set -g prefix C-a
set -g history-limit 10000
set -g default-terminal "screen-256color"
bind-key a last-window
bind-key C-a send-prefix
set -g base-index 1
set -s escape-time 0
set default-path "$PWD"
bind '"' split-window
bind % split-window -h
bind c new-window
set -g default-terminal "screen-256color"
